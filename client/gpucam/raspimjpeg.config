sharpness 0
contrast 0
brightness 50
saturation 0
iso 0
metering_mode average
video_stabilisation false
exposure_compensation 0
exposure_mode auto
white_balance auto
image_effect none
colour_effect_en false
colour_effect_u 128
colour_effect_v 128
rotation 0
hflip false
vflip false
sensor_region_x 0
sensor_region_y 0
sensor_region_w 65536
sensor_region_h 65536
shutter_speed 0
raw_layer false

#
# Preview Options
#
# fps_preview = video_fps (below) / divider
#
width 512
#width 1280
# 10 = 75%
quality 10
divider 1

#
# Video Options
#
video_width 1280
video_height 720
#video_width 1920
#video_height 1080
#video_width 320
#video_height 240
video_fps 10
video_bitrate 17000000
MP4Box true
MP4Box_fps 25

#
# Image Options
#
image_width 2592
image_height 1944
# 10 = 75%
image_quality 10

#
# File Locations
#
# preview path: add %d for number
# image+video path: add %d for number, year, month, day, hour, minute, second
#
#preview_path /dev/shm/mjpeg/cam_prev%02d.jpg
#image_path /dev/shm/mjpeg/cam_full%02d.jpg
preview_path /dev/shm/mjpeg/cam_prev.jpg
image_path /dev/shm/mjpeg/cam_full.jpg

################################
#  Config File for raspimjpeg  #
################################
# annotation max length: 31 characters
annotation Kitchen %04d.%02d.%02d_%02d:%02d:%02d
anno_background false
