<?xml version="1.0" encoding="UTF-8"?>
<plugin xmlns="http://apache.org/cordova/ns/plugins/1.0"
    xmlns:android="http://schemas.android.com/apk/res/android"
    id="com.matd.GetLogin"
    version="0.2.11">
    <name>GetLogin</name>
    <description>The coolest Plugin ever implemented</description>
    <license>Apache 2.0</license>
    <keywords>cordova,coolest</keywords>
    <repo></repo>
    <issue></issue>
    <js-module src="www/GetLogin.js" name="GetLogin">
        <clobbers target="GetLogin" />
    </js-module>
    <!-- android -->
    <platform name="android">
        <config-file target="res/xml/config.xml" parent="/*">
            <feature name="GetLogin" >
                <param name="android-package" value="GetLogin"/>
            </feature>
        </config-file>
        <source-file src="src/org/apache/cordova/plugin" target-dir="src/" />
    </platform>
</plugin>
